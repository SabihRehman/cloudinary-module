{
  "name": "@nuxtjs/cloudinary",
  "version": "1.0.1",
  "description": "Cloudinary module for Nuxt",
  "repository": "nuxt-community/cloudinary-module",
  "license": "MIT",
  "sideEffects": false,
  "keywords": [
    "nuxt",
    "module",
    "nuxt-cloudinary",
    "cloudinary",
    "images",
    "videos",
    "optimization",
    "plugins"
  ],
  "contributors": [
    {
      "name": "Maya Shavin <dpnminh@gmail.com>"
    }
  ],
  "files": [
    "dist",
    "templates"
  ],
  "main": "dist/module.js",  
  "types": "dist/module.d.ts",
  "scripts": {
    "build": "siroc build",
    "dev": "nuxt-ts example",
    "lint": "eslint --ext .js,.ts,.vue .",
    "release": "yarn build && yarn test && standard-version && git push --follow-tags && npm publish",
    "test": "yarn lint && jest",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test"
  },
  "dependencies": {
    "cloudinary": "^1.23.0",
    "cloudinary-build-url": "^0.0.5",
    "cloudinary-vue": "^1.1.2",
    "defu": "^2.0.4"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "latest",
    "@babel/core": "latest",
    "@babel/preset-typescript": "latest",
    "@babel/preset-env": "latest",
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "@nuxt/test-utils": "latest",
    "@nuxtjs/eslint-config-typescript": "latest",
    "@nuxt/typescript-runtime": "latest",
    "@nuxtjs/module-test-utils": "latest",
    "@siroc/jest-preset": "latest",
    "@types/jest": "latest",
    "babel-eslint": "latest",
    "babel-jest": "latest",
    "eslint": "latest",
    "husky": "latest",
    "jest": "latest",
    "nuxt": "latest",
    "puppeteer": "^5.5.0",
    "standard-version": "latest",
    "siroc": "latest",
    "tib": "^0.7.4"
  },
  "peerDependencies": {
    "chalk": "^3.0.0",
    "consola": "^2.15.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "gitHooks": {
      "pre-commit": "lint-staged"
    }
  }
}
